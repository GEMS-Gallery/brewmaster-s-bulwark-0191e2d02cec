type TalentTree = 
 record {
   name: text;
   talents: vec Talent;
 };
type Talent = 
 record {
   column: nat;
   description: text;
   id: text;
   name: text;
   recommended: bool;
   row: nat;
 };
type GuideSection = 
 record {
   content: text;
   title: text;
 };
service : {
  getCooldowns: () -> (vec GuideSection) query;
  getRotation: () -> (vec GuideSection) query;
  getStatPriority: () -> (vec GuideSection) query;
  getTalentTrees: () -> (vec TalentTree) query;
}
