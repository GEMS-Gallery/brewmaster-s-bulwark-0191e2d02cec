type TalentTree = 
 record {
   name: text;
   talents: vec GuideSection;
 };
type GuideSection = 
 record {
   content: text;
   title: text;
 };
service : {
  getCooldowns: () -> (vec GuideSection) query;
  getRotation: () -> (vec GuideSection) query;
  getStatPriority: () -> (vec GuideSection) query;
  getTalentPoints: () -> (vec TalentTree) query;
}
